[user]
	name = ross creighton
	email = rosson.creighton@gmail.com
[difftool]
  prompt = false
[color]
  ui = true
[alias]
  s = status
  a = add
  c = commit -m
  ca = commit --amend
  co = checkout
  f = fetch
  r = reset
  rh = reset --hard
  rs = reset --soft
  d = difftool
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr, %cn)%Creset' --abbrev-commit --date=relative
[push]
  default = current
[pull]
  default = current
[core]
  editor = 'vim'
	excludesfile = /Users/rosscreighton/.gitignore
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/rosscreighton/.stCommitMsg
